{
  "name": "bad-civilization",
  "version": "0.1.1",
  "main": "./App/index.ts",
  "private": true,
  "engines": {
    "node": "^16",
    "yarn": "^3"
  },
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./App/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only ./App/index.ts"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "bignumber.js": "^9.0.2",
    "dayjs": "^1.11.3",
    "dedent": "^0.7.0",
    "discord-api-types": "^0.33.0",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "seedrandom": "^3.0.5",
    "string-similarity": "^4.0.4",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/seedrandom": "^3.0.2",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@yarnpkg/sdks": "^3.0.0-rc.9",
    "cross-env": "^7.0.3",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.2"
  },
  "lint-staged": {
    "*.{cjs,js,ts}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write ."
    ]
  },
  "prettier": {
    "endOfLine": "crlf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
